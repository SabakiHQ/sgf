{
  "name": "@sabaki/sgf",
  "version": "3.4.7",
  "description": "A library for parsing SGF files.",
  "main": "./src/main.js",
  "scripts": {
    "test": "tap tests/*.test.js",
    "format": "prettier \"**/*.{js,md}\" --write",
    "format-watch": "onchange \"**/*.{js,md}\" -- prettier --write {{changed}}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SabakiHQ/sgf.git"
  },
  "keywords": [
    "sgf",
    "go",
    "baduk",
    "weiqi",
    "boardgame",
    "parser",
    "file"
  ],
  "author": "Yichuan Shen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SabakiHQ/sgf/issues"
  },
  "homepage": "https://github.com/SabakiHQ/sgf",
  "dependencies": {
    "doken": "^1.0.0"
  },
  "devDependencies": {
    "iconv-lite": "^0.4.24",
    "jschardet": "^2.1.0",
    "onchange": "^6.1.0",
    "prettier": "1.19.1",
    "tap": "^14.10.6"
  },
  "optionalDependencies": {
    "iconv-lite": "0.4.x",
    "jschardet": ">=1.0 <3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "proseWrap": "always"
  }
}
